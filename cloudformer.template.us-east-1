        "ActionsEnabled": "true",
        "ComparisonOperator": "GreaterThanThreshold",
        "EvaluationPeriods": "1",
        "MetricName": "CPUUtilization",
        "Namespace": "AWS/EC2",
        "Period": "300",
        "Statistic": "Average",
        "Threshold": "80.0",
        "AlarmActions": [
          "arn:aws:sns:us-east-1:035296091979:administrator-us-east-1"
        ],
        "Dimensions": [
          {
            "Name": "InstanceId",
            "Value": { "Ref": "blueeyes" }
          }
        ]
      }
    },
